{
    "name": "mailerlite/mailerlite-api-v2-php-sdk",
    "description": "MailerLite API v2 PHP SDK",
    "homepage": "https://github.com/mailerlite/mailerlite-api-v2-php-sdk",
    "license": "MIT",
    "keywords": ["mailerlite", "sdk", "email", "marketing"],
    "type": "library",
    "require": {
        "php" : "^5.5|^7.0",
        "guzzlehttp/psr7": "~1.2",
        "php-http/httplug": "^1.0",
        "php-http/curl-client": "^1.4",
        "php-http/message": "^1.2"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "5.3.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "MailerLiteApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MailerLiteApi\\Tests\\": "tests/"
        }
    }
}
