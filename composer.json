{
    "name": "mailerlite/mailerlite-api-v2-php-sdk",
    "description": "MailerLite API v2 PHP SDK",
    "homepage": "https://github.com/mailerlite/mailerlite-api-v2-php-sdk",
    "license": "MIT",
    "keywords": ["mailerlite", "sdk", "email", "marketing"],
    "type": "library",
    "authors": [
        {
            "name": "Justinas Pošiūnas",
            "email": "justinas.posiunas@mailerlite.com",
            "homepage": "https://mailerlite.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.5|^7.0",
        "guzzlehttp/psr7": "~1.2",
        "php-http/httplug": "^2.0",
        "php-http/curl-client": "^2.1",
        "php-http/message": "^1.2"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^2.0",
        "phpunit/phpunit": "5.3.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "MailerLiteApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MailerLiteApi\\Tests\\": "tests/"
        }
    }
}
